{
    "name": "ksquaredcoding/proxy",
    "description": "Set trusted proxies for Laravel",
    "keywords": ["proxy", "trusted proxy", "load balancing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Fidao",
            "email": "fideloper@gmail.com"
        },
        {
            "name": "Kevin Kelley",
            "email": "ksquaredcoding@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.5.8|^9.3.3|^10.0"
    },
    "autoload": {
        "psr-4": {
            "KsquaredCoding\\Proxy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KsquaredCoding\\Proxy\\TrustedProxyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
